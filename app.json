{
  "name": "smpalazhar",
  "displayName": "smpalazhar",
  "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "sofwan"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "AlAzhar.apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
}